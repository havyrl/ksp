@PART[desacopladorsat1]:FOR[ProceduralFairings]
{
    %category = Aero
	InterstageBase = TRUE
    !mesh = DELETE
	MODEL
    {
        model = AIES_Aerospace/Structure/AIESdesacoplador sat1/model
        scale = 1.7337,1,1.7337
        position = 0,0,0
    }
	%rescaleFactor = 1
    %scale = 1
    
    %node_stack_top = 0.0, 0.119, 0.0, 0.0, 1.0, 0.0, 0
    %node_stack_top1 = 0.0, 0.4, 0.0, 0.0, 1.0, 0.0, 1
	%node_stack_bottom = 0.0, -0.037, 0.0, 0.0, -1.0, 0.0, 1
    
    @title = IB - AIES SAT-1 Interstage Base
    @description = Open structure interstage base for hot-ignition stages.
	@mass = 0
    !MODULE,* {}
}
